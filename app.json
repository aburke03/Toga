{
  "expo": {
    "name": "Toga",
    "slug": "Toga",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/toga.png", 
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/toga.png" 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/toga.png",  
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/toga.png"  
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
